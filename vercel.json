{
  "version": 2,
  "builds": [
    {
      "src": "frontend/finsage-ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/finsage-ui/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://finsage-backend.railway.app"
  }
}
